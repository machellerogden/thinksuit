{
  "name": "thinksuit-tty",
  "version": "0.1.1",
  "private": true,
  "description": "Terminal component and TTY WebSocket server for ThinkSuit",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/machellerogden/thinksuit.git",
    "directory": "packages/thinksuit-tty"
  },
  "bugs": {
    "url": "https://github.com/machellerogden/thinksuit/issues"
  },
  "homepage": "https://github.com/machellerogden/thinksuit/tree/main/packages/thinksuit-tty#readme",
  "main": "./lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./Terminal.svelte": "./lib/Terminal.svelte",
    "./server": "./server/index.mjs",
    "./style.css": "./lib/index.css"
  },
  "bin": {
    "thinksuit-tty-service": "bin/service.mjs",
    "thinksuit-tty-service-init": "bin/service.init.sh",
    "thinksuit-tty-service-load": "bin/service.load.sh",
    "thinksuit-tty-service-unload": "bin/service.unload.sh",
    "thinksuit-tty-service-start": "bin/service.start.sh",
    "thinksuit-tty-service-stop": "bin/service.stop.sh",
    "thinksuit-tty-service-kill": "bin/service.kill.sh",
    "thinksuit-tty-service-logs": "bin/service.logs.sh",
    "thinksuit-tty-service-info": "bin/service.info.sh"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "vite build",
    "dev": "node ./bin/service.mjs",
    "dev:ui": "vite build --watch",
    "test": "vitest --config ../../vitest.config.js"
  },
  "keywords": [
    "thinksuit",
    "terminal",
    "tty",
    "xterm"
  ],
  "author": {
    "name": "Mac Heller-Ogden",
    "email": "mac@machellerogden.com"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@homebridge/node-pty-prebuilt-multiarch": "^0.11.13",
    "@xterm/addon-attach": "^0.11.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-unicode11": "^0.8.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "express": "^4.19.2",
    "ws": "^8.17.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "svelte": "^5.0.0",
    "vite": "^7.0.4"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  }
}
